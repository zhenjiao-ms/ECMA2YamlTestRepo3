{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "add/includes/**/*.md",
          "add/metadata/**/*.md",
          "**/obj/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "add/codesnippet/**/*.*",
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/smalldotnet/toc.json"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "smalldotnet"
  }
}